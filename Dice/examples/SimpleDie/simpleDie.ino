/* * Dice Beispielprogramm fuer die Dice Library */#include "dice.h"/* * Beispielanordnung: * LED-Nr.     |  Pin-Nr.  | *  0   1      |  7   4    | *  4 6 5      |  8 2 5    | *  3   2      |  6   9    | * */Dice d(7,4,9,6,8,5,2);unsigned char buttonState = 0;void setup() {  d.animation(10);  // Knopf  pinMode(3, INPUT_PULLUP);  // Random Seed setzen  randomSeed(analogRead(0));}void loop() {  buttonState = digitalRead(3);  if (buttonState == LOW) {    d.showNumber(random(1,8));  }}